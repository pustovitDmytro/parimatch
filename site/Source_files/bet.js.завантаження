function top10AnchorLookup()
{var h=document.location.hash;if('undefined'==typeof h||''==h)return;h=h.substr(1);var n=Number(h);if(isNaN(n))return;var links=document.getElementsByTagName('a');var i=0;var a=null;var s='';var ss='';for(i=0;i<links.length;i++)
{a=links[i];if('undefined'==typeof a.id||'undefined'==typeof a.href)continue;s=a.id+'';if(-1==s.indexOf('_'))continue;s=s.substr(1);ss=s.substr(0,s.indexOf('_'));if(n==Number(ss))
{if(document.all)
{a.parentNode.innerHTML+='<a name="'+ss+'a"></a>';}
else
{a.name=ss+"a";}
document.location.hash=ss+"a";return;}}}
Stakes=new(Array);function Stake(id,value){var pos1=0;pos2=0;this.id=id;this.value=value;pos2=value.indexOf('_',pos1);this.name=value.substring(pos1,pos2);pos1=pos2+1;pos2=value.indexOf('_',pos1);this.coef=parseFloat(value.substring(pos1,pos2))/100;pos1=pos2+1;this.data=value.substring(pos1,100);}
function StakesAdd(obj){Stakes[Stakes.length]=obj;}
function StakesIndexOf(stake_id){for(i=0;i<Stakes.length;i++){if(Stakes[i].id==stake_id)return(i);}
return(-1);}
function StakesIndexOfValue(stake_value){for(i=0;i<Stakes.length;i++){if(Stakes[i].value==stake_value)return(i);}
return(-1);}
function StakesIndexOfName(stake_name){for(i=0;i<Stakes.length;i++){if(Stakes[i].name==stake_name)return(i);}
return(-1);}
function StakesDelete(nr){for(;nr<Stakes.length;nr++){Stakes[nr]=Stakes[nr+1];}
Stakes.length=Stakes.length-1;}
function StakesClear(){for(i=0;i<Stakes.length;i++){o=document.getElementById(Stakes[i].value);if(o){$(o).removeClass('active').parent().removeClass('active');}}
Stakes.length=0;}
function StakesKE(){var KE=1;for(i=0;i<Stakes.length;i++){KE*=Stakes[i].coef;}
if(KE==1)return('0');else{KE=Math.round(KE*1000)/1000;if(KE%1==0){KE+=".0"};return(KE);}}
function ShowKE(){if(document.getElementById){k=StakesKE();bc=document.getElementById("betsCount");if(bc)bc.innerHTML=Stakes.length;bc=document.getElementById("betsCoeff");if(bc)bc.innerHTML=k;}}
function m(obj){var name=obj.id.substring(0,obj.id.indexOf('_'));var n=StakesIndexOfValue(obj.id);if(n>=0){StakesDelete(n);$(obj).removeClass('active').parent().removeClass('active');obj.blur();}else{n=StakesIndexOfName(name);if(n>=0){var o=document.getElementById(Stakes[n].value);if(o)
{o.className='';$(o).removeClass('active').parent().removeClass('active');obj.blur();}
StakesDelete(n);}
StakesAdd(new Stake(name,obj.id));obj.className='active';$(obj).addClass('active').parent().addClass('active');obj.blur();}
ShowKE();if($("#click1On").attr('checked')){MS();}
return false;}
