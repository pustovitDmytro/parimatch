var minus='minus';var plus='plus';function toggleProps(propsId,imgId,imgName){var s=$(imgId).attr('src');var container=propsId+' tbody.props';if(imgName==minus){BindOddHandlers(container);$(container).show();s=s.replace(plus,minus);}else
if(imgName==plus){$(container).hide();s=s.replace(minus,plus);}else{if(s.match(plus)){BindOddHandlers(container);$(container).show();s=s.replace(plus,minus);}else{$(container).hide();s=s.replace(minus,plus);}}
$(imgId).attr('src',s);}
function switchProps(){if($('#do_show').is(':visible')){$('table.twp').each(function(){toggleProps('#'+this.id,'#'+this.id.replace('g','i'),minus);});$('#do_show').hide();$('#do_hide').show();}else{$('table.twp').each(function(){toggleProps('#'+this.id,'#'+this.id.replace('g','i'),plus);});$('#do_hide').hide();$('#do_show').show();}}
function toggleMatch(el){var container='#'+el.id.replace('a','p');BindOddHandlers(container);$(container).toggle();}
function prepareBmMenu(){if(jQuery().qtip){$('#bmButton').qtip({content:$('#bmMenu'),show:{when:'click',delay:0,solo:true},hide:{when:{event:'click'},delay:0,fixed:true,effect:{length:0}},position:{target:$('#betMode'),corner:{target:'bottomMiddle',tooltip:'topMiddle'},adjust:{x:1,effect:false}},style:{padding:0,border:{width:1,radius:1,color:'#212121'},classes:{content:'bmMenu'}}});};if($('#betMode span').attr('value')==undefined){$('#betMode span').attr('value',$.cookie('bmValue'));if($('#betMode span').attr('value')==''){$('#betMode span').attr('value',1);}
activeChoice='#bmMenu li a[value="'+ $('#betMode span').attr('value')+'"]';$(activeChoice).addClass('active');$('#betMode span').html($(activeChoice).html());}}
