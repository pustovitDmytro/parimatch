getARContent();});$('#bmMenu a').click(function(){$('#betMode span').html($(this).html());$('#betMode span').attr('value',$(this).attr('value'));$('#bmButton').qtip('hide');$('#bmMenu a').removeClass('active');$(this).addClass('active');$.cookie('bmValue',$(this).attr('value'),{expires:30});});prepareBmMenu();}