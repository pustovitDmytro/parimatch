function getARContent(){var xmlhttp=null;url=window.location.protocol+'//';url+=window.location.host;url+=window.location.port;url+=window.location.pathname.replace('bet.html','live_ar.html');url+=window.location.search;if($(evlist).length==1)
{url+='&he=';$("*:checked",evlist).each(function(){url+=this.value+',';})
if(typeof Curs=='number'){url+='&curs='+ Curs;url+='&curName='+ CurName;}}
$('#noData').remove();if(window.XMLHttpRequest){xmlhttp=new XMLHttpRequest();}else if(window.ActiveXObject){xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");}
if(!xmlhttp)return;xmlhttp.open("GET",url,true);xmlhttp.onreadystatechange=function()
{var req=xmlhttp;var nid='oddsList';if(req.readyState==4)
{if(req.status==200)
{var e=document.getElementById(nid);responseText=CompareOdds(e,req.responseText);if('undefined'!=typeof e)
{UnbindOddHandlers('#z_contentw table.dt');lastDisplay=$(".wrapper",evlist).css("display");e.innerHTML=responseText;if(!$(e).hasClass('updated'))
{$(e).addClass('updated');}
$(".wrapper",evlist).css('display',lastDisplay);if(lastDisplay=='none'){$("h3 img","#evList").attr({src:"img/live_plus.png"});}else{$("h3 img","#evList").attr({src:"img/live_minus.png"});$("#evFiller").hide();}
BindOddHandlers('#z_contentw table.dt');BindMatchInfo();BindTooltips();BindVLink();$(":checkbox",evlist).click(function(){if(!isReqSended)
getARContent();});}}else{BlankOdds();}
isReqSended=false;}};isReqSended=true;xmlhttp.send('');};function CompareOdds(aPrev,aNow){prev=$(aPrev);now=$('<div>'+aNow+'</div>').find('#oddsList');t1=new Date();prev.find('a i').each(function(ix,el){if(el.id){t2=new Date();tdiff=t2.getTime()- t1.getTime();if((tdiff>1500)){return(false);}
elNow=now.find('#'+el.id);prevCf=parseFloat(el.innerHTML);curCf=parseFloat(elNow.html());if(prevCf>curCf)
curClass='down'
else if(prevCf<curCf)
curClass='up'
else
curClass=null;if(curClass!=null){elNow.removeClass().addClass(curClass);}
if(ix%50==0){setTimeout(dummy(ix),0);}}});return(now.html());}
